<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/pro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ViewFlipper
            android:id="@+id/imageSwitcher2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imagePlayer1_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p10" />

            <ImageView
                android:id="@+id/imagePlayer2_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p9" />

            <ImageView
                android:id="@+id/imagePlayer3_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p8" />

            <ImageView
                android:id="@+id/imagePlayer4_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p7" />

            <ImageView
                android:id="@+id/imagePlayer5_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p6" />

            <ImageView
                android:id="@+id/imagePlayer6_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p5" />

            <ImageView
                android:id="@+id/imagePlayer7_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p4" />

            <ImageView
                android:id="@+id/imagePlayer8_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p3" />

            <ImageView
                android:id="@+id/imagePlayer9_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p2" />

            <ImageView
                android:id="@+id/imagePlayer10_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="center"
                android:src="@drawable/p1" />
        </ViewFlipper>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/common"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ViewFlipper
            android:id="@+id/imageSwitcher"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="50dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imagePlayer1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o1min" />

            <ImageView
                android:id="@+id/imagePlayer2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o2min" />

            <ImageView
                android:id="@+id/imagePlayer3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o3min" />

            <ImageView
                android:id="@+id/imagePlayer4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o4min" />

            <ImageView
                android:id="@+id/imagePlayer5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o5min" />

            <ImageView
                android:id="@+id/imagePlayer6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o6min" />

            <ImageView
                android:id="@+id/imagePlayer7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:adjustViewBounds="true"
                android:background="@color/colorPrimaryDark"
                android:cropToPadding="false"
                android:scaleType="centerInside"
                android:src="@drawable/o7min" />
        </ViewFlipper>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="13dp"
        android:background="#00000000"
        android:text="@string/_0"
        android:textColor="#7F7F7F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:typeface="monospace"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/corky"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:paddingEnd="-5dp"
        android:paddingRight="-5dp"
        android:text="@string/menu"
        android:textColor="#7F7F7F"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"

        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-3254112346644116/8075397344"
        app:layout_anchorGravity="bottom|center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/buyButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/transparent"
        android:paddingTop="11dp"
        android:src="@drawable/adbutton"

        android:tint="#7F7F7F"
        app:layout_anchorGravity="top|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>